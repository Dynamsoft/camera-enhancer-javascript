{
	"name": "dynamsoft-camera-enhancer",
  "version": "4.0.0",
	"description": "Allow your website to easily control cameras on desktop and mobile devices.",
	"private": false,
	"homepage": "https://www.dynamsoft.com/camera-enhancer/docs/programming/javascript/?ver=latest",
	"main": "dist/dce.js",
	"module": "dist/dce.mjs",
	"exports": {
		"import": {
			"types": "./dist/types/index.d.ts",
			"default": "./dist/dce.esm.js"
		},
		"require": {
			"types": "./dist/types/index.d.ts",
			"default": "./dist/dce.js"
		}
	},
	"sideEffects": false,
	"types": "./dist/types/index.d.ts",
	"publishConfig": {
		"registry": "https://registry.npmjs.org/"
	},
	"author": {
		"name": "Dynamsoft",
		"url": "https://www.dynamsoft.com"
	},
	"maintainers": [
		{
			"name": "Dynamsoft",
			"email": "support@dynamsoft.com"
		}
	],
	"keywords": [
		"dynamsoft",
		"camera control",
		"camera enhancer",
		"javascript camera",
		"sdk",
		"javascript",
		"web"
	],
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"rollup-dev": "rollup -c --environment BUILD:development",
		"rollup": "rollup -c --environment BUILD:production",
		"build-dev": "python ./build-dev.py",
		"build": "python ./build.py",
		"build:doc": "typedoc",
		"update:readme": "node ./updateReadme.js"
	},
	"license": "SEE LICENSE IN LICENSE",
	"repository": {
		"type": "git",
		"url": "https://github.com/Dynamsoft/camera-enhancer-javascript.git"
	},
	"files": [
		"/dist/*.js",
		"/dist/*.mjs",
		"/dist/*.html",
		"/dist/*.wasm",
		"/dist/**/*.d.ts",
		"/API Reference.url",
		"/LEGAL.txt",
		"/samples.url"
	],
	"devDependencies": {
		"@dynamsoft/camera-manager": "^1.1.2",
		"@dynamsoft/tools": "^0.0.1",
		"@rollup/plugin-node-resolve": "^13.3.0",
		"@rollup/plugin-replace": "^5.0.2",
		"@rollup/plugin-terser": "^0.4.3",
		"@rollup/plugin-typescript": "^11.1.2",
		"@scannerproxy/browser-info": "^2.0.8",
		"@scannerproxy/curscript-path": "^2.0.1",
		"@types/node": "^20.4.2",
		"eruda": "^3.0.1",
		"highlight.js": "^9.15.6",
		"mutable-promise": "^1.1.13",
		"rollup": "^2.79.1",
		"showdown": "^1.9.0",
		"tslib": "^2.5.0",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"dm-fabric": "^5.1.17",
		"dm-howler": "^2.2.4",
		"dynamsoft-capture-vision-router": "^2.0.11",
		"dynamsoft-core": "^3.0.10"
	}
}
